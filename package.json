{
  "name": "portal-api",
  "version": "1.0.0-rc.1",
  "description": "Portal API - Tenant-aware API for contacts, jobs, and files",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:real": "tsx watch src/index-real.ts",
    "dev:gate-c": "tsx watch src/index-gate-c.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:real": "node dist/index-real.js",
    "test": "jest",
    "test:real": "jest --testMatch='**/tests/real/**/*.test.ts'",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["api", "portal", "tenant"],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2"
  }
}