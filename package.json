{
  "name": "portal-api",
  "version": "1.0.0-rc.1",
  "description": "Portal API - Tenant-aware API for contacts, jobs, and files",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:real": "tsx watch src/index-real.ts",
    "dev:gate-c": "tsx watch src/index-gate-c.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:real": "node dist/index-real.js",
    "test": "jest",
    "test:real": "jest --testMatch='**/tests/real/**/*.test.ts'",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "api",
    "portal",
    "tenant"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@azure/identity": "^4.2.1",
    "@azure/keyvault-secrets": "^4.8.0",
    "@supabase/supabase-js": "^2.56.0",
    "applicationinsights": "^2.9.5",
    "argon2": "^0.40.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.9.2"
  }
}
