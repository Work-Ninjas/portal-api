# Server Configuration
PORT=3000
NODE_ENV=development

# API Configuration
API_VERSION=v1
API_PREFIX=/v1

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=60

# CORS
CORS_ORIGIN=*

# Logging
LOG_LEVEL=info

# RPC Configuration (for future use)
RPC_BASE_URL=http://localhost:8080
RPC_TIMEOUT_MS=10000